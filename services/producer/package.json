{
  "name": "@redis/producer",
  "version": "0.0.1",
  "description": "Redis Producer",
  "main": "dist/server.js",
  "repository": "git@github.com:houseoftypescript/messages.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "GNU GPL 3.0",
  "private": true,
  "engines": {
    "node": "20.1.0",
    "npm": "9.6.6",
    "yarn": "1.22.19"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "serve": "kill-port 4000 && concurrently \"nodemon\" \"nodemon -x yarn docs\"",
    "docs": "yarn docs:swagger:json && yarn docs:swagger:yaml && yarn docs:postman",
    "docs:postman": "node ./scripts/postman",
    "docs:swagger:json": "tsoa spec-and-routes",
    "docs:swagger:yaml": "json2yaml ./docs/apis/swagger.json > ./docs/apis/swagger.yaml",
    "lint": "eslint . --fix --ext .ts",
    "start": "node .",
    "test": "jest",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "ioredis": "5.3.2",
    "pino": "8.14.1",
    "tsoa": "5.1.1"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/http-errors": "2.0.1",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.6",
    "@types/webpack-node-externals": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "eslint": "8.40.0",
    "jest": "29.5.0",
    "json2yaml": "1.1.0",
    "kill-port": "2.0.1",
    "nodemon": "2.0.22",
    "rimraf": "5.0.0",
    "swagger2-to-postmanv2": "2.1.0",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "webpack": "5.82.0",
    "webpack-cli": "5.1.1",
    "webpack-node-externals": "3.0.0"
  }
}
